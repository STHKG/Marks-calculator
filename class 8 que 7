
#include<stdio.h>
#include<stdbool.h>
void main()
{
int i,s1=0,s2=0,s3=0,s4=0,c,d;
int arr[5];

for(i=0;i<5;i++)
	{
	scanf("%d",&arr[i]);
	}

for(i=0;i<5;i++)
	{
	if(arr[i]%2==0)
		 s1=s1+arr[i];
	else s2=s2+arr[i];
	
		
	if(i%2==0)
		 s3=s3+arr[i];
	else s4=s4+arr[i];
	}



printf("The sum of odd values:%d even values: %d\n",s2,s1);
printf("The sum of odd indexes: %d even indexes:%d\n",s4,s3);

}
